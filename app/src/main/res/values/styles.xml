<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Toolbar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:background">@color/toolbar</item>
    </style>

    <style name="main_Abottom" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="android:fontFamily">@font/sans_prob</item>
        <item name="android:textColor">@color/main_textcolor_bottom</item>
    </style>

    <style name="tablayout" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textSize">35sp</item>
    </style>

</resources>
